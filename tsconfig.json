{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "types": ["node"]
  },
  "include": [
    "src",
    "gatsby-*.ts",
    "scripts"
  ],
  "exclude": [
    "node_modules",
    "public",
    ".cache"
  ]
}
